{
  "name": "ts-discord-bot",
  "version": "1.0.0",
  "description": "A template for a discord bot made in Typescript",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build && npx tsc",
    "build-docker": "docker build . -t bot-container",
    "bot": "npm run build && env-cmd node ./build/index.js",
    "bot-docker": "npm run build-docker && docker run --env-file=./.env  bot-container"
  },
  "author": "Fyrebirdd",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/sqlite3": "^3.1.11",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "sqlite3": "^5.1.7"
  },
  "type": "module"
}
