{
  "name": "ts-discord-bot",
  "version": "1.0.0",
  "description": "A template for a discord bot made in Typescript",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build && npx tsc && docker build -t bot-container .",
    "bot": "docker run --env-file=bot.env bot-container",
    "compile": "rimraf build && npx tsc",
    "dbuild": "docker build -t bot-container .",


    "deploy": "node build/deployCommands.js",
    "delete": "node build/deleteCommand.js"    
  },
  "author": "Fyrebirdd",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/sqlite3": "^3.1.11",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.4",
    "sqlite3": "^5.1.7"
  },
  "type": "module"
}
